@font-face{font-family:spacemono;src:url(../fonts/SpaceMono-Regular.ttf) format('truetype')}#canvas{position:fixed;top:0;left:0;width:100%;height:100%;background:#000}#content{pointer-events:none;color:transparent!important;width:0;padding:0;margin:0}body{position:fixed;top:0;left:0;width:100%;height:100%;z-index:1;text-transform:uppercase;font-size:1.4vh;font-family:spacemono}body{background:#000;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}*{-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}#popupJoin{position:fixed;top:50%;left:50%;padding:24px;border-radius:12px;background:#fff;color:#000;text-align:center;transform:translate(-50%,-50%);z-index:2;transition:all .2s ease-in-out;opacity:0;pointer-events:none;max-width:90vw;max-height:80vh}#popupJoin.is-active{opacity:1;pointer-events:all}#popupJoin p{padding:0;margin:0}#popupJoin input{margin:0 .5em 1em;background:transparent;outline:none;border:2px solid #000;padding:6px;border-radius:6px;width:3em;height:3em;cursor:pointer;transition:all .2s ease-in-out}#popupJoin input:hover{background:#000}#popupJoin button{padding:1em 2em;border-radius:1.5em;line-height:1em;background:#fff;color:#000;text-transform:uppercase;border:2px solid #000;outline:none;margin-top:2em;transition:all .2s ease-in-out}#popupJoin button:hover{background:#000!important;color:#fff!important}#outputPlayer{position:fixed;bottom:1em;left:1em;color:#fff;z-index:2}@media(max-aspect-ratio:1/1){#outputPlayer{left:auto;bottom:auto;top:1em;right:1em;text-align:right}}#outputPOI{position:fixed;bottom:1em;right:1em;color:#fff;z-index:2;list-style:none;text-align:right}#logo{position:fixed;top:1rem;left:1rem;z-index:2;width:24px;height:auto;z-index:100}#menu{position:fixed;top:-100vh;height:100vh;width:100vw;background:rgba(0,0,0,.5);transition:all .2s ease-in-out;z-index:100;left:0}#menu #links{margin:0;padding:0;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);text-align:center;font-size:1rem;color:#fff}.menu-open #menu{top:0;background:rgba(0,0,0,.8)}#menu a{color:#fff;text-decoration:none}#menu li{display:block;padding:.5rem;cursor:pointer}#title{position:fixed;top:1em;right:1em;z-index:2;font-size:1em;line-height:1em;height:auto;color:#fff;font-weight:400;text-align:right}#outputPOI a{color:#fff;text-decoration:none;cursor:pointer}table{border-spacing:1em}th,td{min-width:3em;line-height:1em}.color{width:1em;height:1em;display:inline-block}#info{position:fixed;top:-50%;left:50%;background:#000;width:90vw;height:80vh;max-width:640px;max-height:640px;z-index:1000;border:1px solid #fff;transition:all .5s ease-in-out;transform:translate(-50%,-50%);transform-origin:50% 50%;box-shadow:12px 12px #000,-12px 12px #000,12px -12px #000,-12px -12px #000}.info-open #info{top:50%}#canvas{transform:translateY(0%);transition:all .5s ease-in-out}.info-open #canvas{transform:translateY(50%)}.info-inner{position:absolute;top:0;left:0;width:100%;overflow:hidden;overflow-y:hidden;height:calc(100% - 3rem);overflow-y:auto}.info-body{overflow:hidden;overflow-y:auto}.info-footer{position:absolute;bottom:0;left:0;width:100%;border-top:1px solid #fff;height:3rem;background:#000}#infoTitle{color:#fff;font-size:1rem;line-height:1.2em;margin:0;padding:0;padding:1rem;border-bottom:1px solid #fff}#infoArtists{color:#fff;font-size:1rem;line-height:1.2em;margin:0;padding:0;padding:1rem;border-bottom:1px solid #fff}#infoDate{color:#fff;font-size:1rem;line-height:1em;padding:1rem;border-bottom:1px solid #fff}#infoDesc{color:#fff;font-size:1rem;line-height:1.5em;padding:1rem;text-transform:none}#infoDesc a{color:#fff}#infoOpen{width:calc(50% - 2rem);position:absolute;right:0;bottom:0;height:1rem;font-size:1rem;line-height:1em;text-decoration:none;padding:1rem;text-align:center;background:transparent;border:0;outline:none;font-family:spacemono;color:#fff;text-transform:uppercase;cursor:pointer;border-left:1px solid #fff;background:#fff;color:#000}#info.no-link #infoOpen{display:none}#infoExit{font-size:1rem;line-height:1em;text-decoration:none;width:calc(50% - 2rem);position:absolute;left:0;bottom:0;height:1rem;padding:1rem;text-align:center;background:transparent;border:0;outline:none;font-family:spacemono;color:#fff;text-transform:uppercase;cursor:pointer}#info.no-link #infoExit{width:calc(100% - 2rem)}#tutorial{font-family:spacemono;color:#fff;font-size:1rem;line-height:1em;position:fixed;top:20%;left:50%;transform:translate(-50%,-100vh);max-width:640px;width:80vw;text-align:center;z-index:10;background:#000;padding:1em;opacity:0;pointer-events:none;transition:all .5s ease-in-out;border:2px solid #fff;box-shadow:12px 12px #000,-12px 12px #000,12px -12px #000,-12px -12px #000}.tutorial-open #tutorial{transform:translate(-50%,0);opacity:1}#logos{position:absolute;bottom:1rem;left:1rem;width:calc(100% - 2rem);height:72px;list-style:none;margin:0;padding:0;background-image:url(../img/banner_small.jpg);background-size:auto 72px;background-position:0 0;background-repeat:repeat-x;animation:banner 60s linear infinite;animation-play-state:running}#logos:hover{animation-play-state:paused}@keyframes banner{0%{background-position:0 0}100%{background-position:-100% 0}}#vierterRaumCurtain{position:fixed;top:50%;left:50%;transform:translate(-50%,-50%);width:0%;height:0%;outline:10000px solid #000;z-index:1000000;transition:all 1s ease-in-out;pointer-events:none;-webkit-user-select:none;-moz-user-select:none;-ms-user-select:none;user-select:none}.curtain-open #vierterRaumCurtain{width:100%;height:100%}#menubutton{position:fixed;top:1rem;right:1rem;background:transparent;width:24px;height:24px;z-index:100;border:0;outline:none;cursor:pointer}#menubutton .menubutton-inner{position:absolute;top:50%;left:0;width:24px;height:4px;background:#fff;transform:translateY(-50%);transition:all .2s ease-in-out}#menubutton:before{content:"";position:absolute;top:2px;left:0;width:24px;height:4px;background:#fff;transition:all .2s ease-in-out}#menu